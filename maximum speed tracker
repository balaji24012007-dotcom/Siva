#include <stdio.h>

int main() {
    int n;
    float speed, maxSpeed = 0.0;

    printf("Enter number of speed readings: ");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        printf("Enter speed %d: ", i + 1);
        scanf("%f", &speed);

        if (speed > maxSpeed) {
            maxSpeed = speed;
        }
    }

    printf("\nMaximum speed recorded: %.2f km/h\n", maxSpeed);

    return 0;
}

#include <stdio.h>
#include <string.h>

int main() {
    char adminUser[] = "admin";
    char adminPass[] = "1234";

    char user[20], pass[20];

    printf("Enter username: ");
    scanf("%s", user);

    printf("Enter password: ");
    scanf("%s", pass);

    if (strcmp(user, adminUser) == 0 && strcmp(pass, adminPass) == 0) {
        printf("Login successful\n");
    } else {
        printf("Invalid credentials\n");
    }

    return 0;
}

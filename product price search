#include <stdio.h>

int main() {
    int n;
    float targetPrice;

    printf("Enter number of products: ");
    scanf("%d", &n);

    float prices[n];

    // Input prices
    for (int i = 0; i < n; i++) {
        printf("Enter price of product %d: ", i + 1);
        scanf("%f", &prices[i]);
    }

    printf("Enter price to search: ");
    scanf("%f", &targetPrice);

    // Linear search
    for (int i = 0; i < n; i++) {
        if (prices[i] == targetPrice) {
            printf("Price found at position %d\n", i + 1);
            return 0;
        }
    }

    printf("Price not found\n");
    return 0;
}

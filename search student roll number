#include <stdio.h>

int main() {
    int n, key;
    printf("Enter number of students: ");
    scanf("%d", &n);

    int roll[n];

    // Input roll numbers
    for (int i = 0; i < n; i++) {
        printf("Enter roll number %d: ", i + 1);
        scanf("%d", &roll[i]);
    }

    printf("Enter roll number to search: ");
    scanf("%d", &key);

    // Linear search
    for (int i = 0; i < n; i++) {
        if (roll[i] == key) {
            printf("Roll number found at position %d\n", i + 1);
            return 0;
        }
    }

    printf("Roll number not found\n");
    return 0;
}
